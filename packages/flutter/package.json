{
  "name": "@flutter-builder/flutter",
  "version": "2.5.0-f8",
  "description": "flutter widget builder from nodejs",
  "main": "dist/index.js",
  "repository": "https://github.com/gridaco/coli-flutter-builder",
  "author": "softmarshmallow <woojoo@softmarshmallow.com>",
  "license": "MIT",
  "private": false,
  "scripts": {
    "copy:README": "cp ../../README.md README.md",
    "prepack": "tsc && yarn copy:README",
    "test": "jest"
  },
  "dependencies": {
    "@abraham/reflection": "^0.8.0",
    "dart-style": "^1.3.2-dev"
  },
  "devDependencies": {
    "@types/assert": "^1.5.6",
    "@types/jest": "^26.0.14",
    "@types/node": "latest",
    "jest": "^26.5.2",
    "ts-jest": "^26.4.1",
    "typescript": "^4.0.5"
  },
  "files": [
    "LICENSE",
    "README.md",
    "dist/index.js",
    "dist/index.d.ts",
    "dist/**/*.d.ts",
    "dist/**/*.js"
  ],
  "jest": {
    "transform": {
      ".(ts|tsx)": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ]
  },
  "publishConfig": {
    "access": "public"
  }
}
