{
  "$schema": "https://turbo.build/schema.json",
  "pipeline": {
    "build": {
      "outputs": [
        "dist/**",
        ".next/**"
      ],
      "dependsOn": [
        "^build"
      ]
    },
    "lint": {},
    "test": {
      "outputs": [],
      "dependsOn": [
        "build"
      ]
    },
    "flutter-preview-webview#dev": {
      "dependsOn": [
        "^build"
      ],
      "outputs": [
        ".next"
      ]
    }
  }
}